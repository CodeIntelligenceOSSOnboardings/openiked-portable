 
{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 25,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "dev-base",
      "inherits": ["base"],
      "cacheVariables": {
        "FUZZER_RUN_JOBS": "16",
        "FUZZER_RUN_WORKERS": "16",
        "FUZZER_RUN_TIME_IN_SECONDS": "10"
      },
      "hidden": true
    },
    {
      "name": "dev-x64-freebsd-llvm-debug",
      "inherits": [ "dev-base", "make", "x64-freebsd", "freebsd-llvm-base", "debug", "coverage" ]
    },
    {
      "name": "dev-x64-freebsd-llvm-release",
      "inherits": [ "dev-base", "make", "x64-freebsd", "freebsd-llvm-base", "release" ]
    },
    {
      "name": "dev-x64-windows-llvm-debug",
      "inherits": [ "dev-base", "vs2022", "x64-windows", "windows-llvm-base", "debug" ]
    }
  ],
  "buildPresets": [
    {
      "name": "dev-x64-freebsd-llvm-debug",
      "configurePreset": "dev-x64-freebsd-llvm-debug",
      "configuration": "Debug",
      "inherits": [ "freebsd" ]
    },
    {
      "name": "dev-x64-freebsd-llvm-release",
      "configurePreset": "dev-x64-freebsd-llvm-release",
      "configuration": "Release",
      "inherits": [ "freebsd" ]
    },
    {
      "name": "dev-x64-windows-llvm-debug",
      "configurePreset": "dev-x64-windows-llvm-debug",
      "configuration": "Debug",
      "inherits": [ "windows" ]
    }
  ],
  "testPresets": [
    {
      "name": "dev-x64-freebsd-llvm-debug",
      "configurePreset": "dev-x64-freebsd-llvm-debug",
      "inherits": [ "ci", "freebsd", "debug" ]
    },
    {
      "name": "dev-x64-freebsd-llvm-release",
      "configurePreset": "dev-x64-freebsd-llvm-release",
      "inherits": [ "ci", "freebsd", "release" ]
    }
  ]
}
